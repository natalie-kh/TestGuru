<p>
  <%= link_to '< Test List', tests_path %>
</p>
<h1> '<%=@test.title.capitalize%>' test</h1>
<% if @questions.present? %>
    <h2>Question List:</h2>
    <table>
      <tr>
        <th>Body:</th>
        <th>Answers count:</th>
        <th colspan="3">Actions</th>
      </tr>
      <%= render @questions %>
<% else %>
    <p>No questions yet</p>
<% end %>
</table>
<p>
  <%= link_to 'Create New Question', new_test_question_path(@test)%>
</p>