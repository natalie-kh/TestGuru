
<p>
  <strong>Title:</strong>
  <%= @test.title%>
</p>

<p>
  <strong>Level:</strong>
  <%= @test.level%>
</p>

<% if @test.questions.present? %>
<h2>Questions</h2>
<% else %>
    <h2>No questions yet</h2>
<% end %>

<p>
  <%= link_to 'New Question', new_test_question_path(@test)%>
</p>

<ol>
  <%#= render @test.questions %>
  <%= render partial: 'questions/question', collection: @test.questions %>
</ol>

<p>
  <%= link_to 'Edit', edit_admin_test_path(@test)%> |
  <%= link_to 'Back', admin_tests_path %>
</p>
